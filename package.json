{
    "name": "broomball-react-app",
    "version": "1.0.0",
    "scripts": {
        "client": "cd client && yarn start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "start": "concurrently \"nodemon server.js\" \"npm run client\""
    },
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "cors": "^2.8.4",
        "express": "^4.16.2",
        "moment": "^2.22.2",
        "mongoose": "^5.2.4",
        "morgan": "^1.9.0",
        "concurrently": "^3.5.0"
    },
    "devDependencies": {
        "concurrently": "^3.5.0"
    }
}
